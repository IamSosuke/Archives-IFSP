algoritmo "ContadorPar"

var
   numero, numb: inteiro
inicio

      numb <- 0
      Para numero <- 10 ate 20 faca
           Se (numero % 2 = 0) entao
              Escreval ("Número Par: ", numero,".")
           numb <- numb + numero


           FimSe
      FimPara
           
      Escreval ("A somatória dos pares é: ",numb,".")






fimalgoritmo

