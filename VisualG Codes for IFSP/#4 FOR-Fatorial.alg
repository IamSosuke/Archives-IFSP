algoritmo "Fatorial"

var
   i, fatorial, valor: inteiro
inicio
      Escreval ("Informe o valor para o cálculo do fatorial: ")
      Leia (valor)
      
      fatorial <- 1
      para i <- 1 ate valor faca
      fatorial <- fatorial * i
      Escreval ("Valor: ",fatorial)
      fimpara
      
      Escreval ("O valor final é: ", fatorial,".")

      














fimalgoritmo
