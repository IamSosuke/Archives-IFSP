algoritmo "ParouImpar"

var
   numero, numb: inteiro

inicio

      Para numero <- 100 ate 200 faca
      
           numb <- numero % 2
      
           Se (numero % 2 <> 0) entao
              Escreval ("Número ",numero," IMPAR.")
              Senao
                   Escreval ("Número ",numero," PAR.")
           
           FimSe
      FimPara
      




















fimalgoritmo

